{
    "name": "gurindersingh/avi",
    "description": "Deploy simple laravel app",
    "keywords": ["framework", "laravel", "deploy", "console", "cli"],
    "homepage": "https://github.com/gurindersingh/avi",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/gurindersingh/avi/issues",
        "source": "https://github.com/gurindersingh/avi"
    },
    "authors": [
        {
            "name": "Gurinder Chauhan",
            "email": "gurinder.in.ca@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel-zero/framework": "^9.0",
        "nunomaduro/termwind": "^1.3",
        "protonemedia/laravel-blade-on-demand": "^1.5",
        "react/child-process": "^0.6.4",
        "react/event-loop": "^1.3",
        "react/promise": "^2.9",
        "spatie/dns": "^2.5",
        "spatie/fork": "^1.1",
        "symfony/process": "^6.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Support/react.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/avi"]
}
